<?xml version="1.0" encoding="UTF-8"?>
<database name="default" package="src.PGS.CoreDomainBundle.Model.StudentMedical" namespace="PGS\CoreDomainBundle\Model\StudentMedical">
    <table name="student_medical" phpName="StudentMedical">
        <column name="school_health_id" type="INTEGER" required="true" primaryKey="true" />
        <column name="medical_id" type="integer" size="5" required="true" primaryKey="true"/>

        <foreign-key foreignTable="school_health" onDelete="CASCADE" onUpdate="CASCADE">
            <reference local="school_health_id" foreign="id"/>
        </foreign-key>
        <foreign-key foreignTable="medical" onDelete="CASCADE" onUpdate="CASCADE">
            <reference local="medical_id" foreign="id"/>
        </foreign-key>
    </table>
</database>
